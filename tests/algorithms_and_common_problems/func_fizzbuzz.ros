functie fizzbuzz(var n) {
    daca (n % 15 == 0) atunci {returneaza "FizzBuzz";}
    altfel daca (n % 3 == 0) atunci {returneaza "Fizz";}
    altfel daca (n % 5 == 0) atunci {returneaza "Buzz";}
    returneaza sirc(n);
}

pentru (var i = 1; i <= 100000; i = i + 1) executa {
    afiseaza(fizzbuzz(i));
}
